    <add key="ResizeQueueName" value="resizerequest" />
    <add key="DeleteQueueName" value="deleterequest" />
    <add key="UploadContainerName" value="images" />
    <add key="FacebookAuthString" value="http://www.facebook.com" />
    <add key="DefaultUserId" value="11111111-1111-1111-1111-111111111111" />
    <add key="DefaultAlbumId" value="11111111-1111-1111-1111-111111111111" />
    <add key="DefaultServiceURL" value="http://localhost:31478" />
    <add key="enableSoftDelete" value="true" />
    <add key="MS_NotificationHubName" value="REPLACE_ME"/>

